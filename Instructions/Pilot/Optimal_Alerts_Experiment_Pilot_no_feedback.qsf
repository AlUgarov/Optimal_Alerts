und_info[3]);\n\t   var hint=hints[parseInt(selround_info[4])];\n       var ch=parseInt(selround_info[5]);\n\t   var ballcolor=ballcolors[state];\n\t   var action=actions[ch];\n\t   Qualtrics.SurveyEngine.setEmbeddedData(\"Final_Payoff\", selround_info[2]);\n\t   var infomess=\"The ball was \"+ballcolor+\". The gremlin said that \"+hint+\". \";\n\t   var infomess=infomess+\"As you chose \"+action+\" in response to this hint, your payoff will be $\"+selround_info[2].toString();\n\t   message=infomess+\" (with the show-up fee included). \"+mess0;  \n   \n   } else if (seltaskN==3){\n\t   var state=parseInt(selround_info[3]);\n\t   var ballcolor=ballcolors[state];\n\t   var hint=hints[parseInt(selround_info[4])];\n       var chance=parseInt(selround_info[5]);\n\t   var rnumber=Math.round(100.0*parseFloat(selround_info[6]));\n\t   var paying=selround_info[7];\n\t   var payoff=parseInt(selround_info[2]);\n\t   Qualtrics.SurveyEngine.setEmbeddedData(\"Final_Payoff\", selround_info[2]);\n\t   \n\t   if (paying==\"Lottery\"){\n\t\t   var comp=\"smaller\";\n\t\t   var reason=\"this random lottery. \";\n\t\t   if (payoff>6){\n\t\t      reason=reason+\"By chance, you did not lose in this lottery\";\n\t\t   } else {\n\t\t\t   reason=reason+\"By chance, you lost $20 in this lotter\";\n\t\t   }\n\t   } else {\n\t\t   var comp=\"greater\";\n\t\t   var reason=\"the actual ball color.\";\n\t\t   if (state==0){\n\t\t\t   reason=reason+\" Because the ball was white, you did not lose $20\";\n\t\t   } else {\n\t\t\t   reason=reason+\" Because the ball was black, you lost $20\";\n\t\t   }\n\t   }\n\n\t   var infomess=\"The ball was \"+ballcolor+\". The gremlin said that \"+hint+\". \";\n\t   var infomess=infomess+\"You estimated that there was a \"+chance+\"% chance that the Ball would be black given the hint. \";\n\t   var infomess=infomess+\"The computer drew a random lottery with a \"+rnumber+\"% chance of losing $20. \"\n\t   var infomess=infomess+\"As this chance was \"+comp+\" than